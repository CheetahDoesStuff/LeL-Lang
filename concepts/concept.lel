var my_variable = 12;
rmv my_variable;

upd my_variable = 13;

upd my_variable = (my_variable + my_variable);


def my_func {
    var new_var = 2;
    upd my_variable = (my_variable - new_var);
};


ifs [iz my_variable] run my_func;

// comment
// {} = function code block
// () = calculation
// [] = comparison / if

